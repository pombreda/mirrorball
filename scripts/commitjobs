"""
Script for committing a set of rmake jobs one at a time.
"""

import sys
import logging

from header import *

log = logging.getLogger('script')

jobIds = sys.argv[2:]

for jobId in jobIds:
    log.info('starting commit of %s' % jobId)
    builder.commit(jobId)
    log.info('commit of %s completed' % jobId)

